#include "holberton.h"
/**
* *_strncat - linking two strings
*
*
*Return: 0
*/
char *_strncat(char *dest, char *src, int n)
{
	char *start = dest;
	int i;

    while(*dest != '\0')
    {
        dest++;
    }

    for (i < n; i++)
    {
        *dest = *src;
        dest++;
        src++;
    }

    *dest = '\0';
    return start;
}
